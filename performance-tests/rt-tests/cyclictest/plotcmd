set title "Latency plot"
set terminal png
set xlabel "Latency (us), max 100 us"
set logscale y
set xrange [0:400]
set yrange [0.8:*]
set ylabel "Number of latency samples"
set output "graphs/ciclictest.png"
plot "cyclictest/histogram1" using 1:2 title " CPU0" with histeps, "cyclictest/histogram2" using 1:2 title " CPU1" with histeps, "cyclictest/histogram3" using 1:2 title " CPU2" with histeps, "cyclictest/histogram4" using 1:2 title " CPU3" with histeps